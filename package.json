{
  "name": "knowledge-is-sexy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-router-dom": "^4.2.6",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.6.0",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.0",
    "tslint-react": "^3.6.0"
  },
  "scripts": {
    "start": "npm run watch-css & npm run start-js",
    "build": "npm run build-css & npm run build-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "8.0.0",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "typescript": "^3.1.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
