{
  "name": "knowledge-is-sexy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-router-dom": "^4.2.6",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.15.1"
  },
  "scripts": {
    "start": "npm run watch-css & npm run start-js",
    "build": "npm run build-css & npm run build-js",
    "start-js": "react-scripts-ts start",
    "build-js": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "8.0.0",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "typescript": "^2.8.3"
  }
}
